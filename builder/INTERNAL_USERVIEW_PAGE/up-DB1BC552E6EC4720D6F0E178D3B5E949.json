            "className": "org.joget.plugin.enterprise.InformationTileComponent",
            "properties": {
                "template": "<!--{sampleWidth: '250px', tags: 'light-bg tag header bar'}-->\r\n<div class=\"card\" \r\n     style=\"height:{{height}};\r\n            background-color:{{bgColor}}; \r\n            background-image:url('{{bgImg}}');\">\r\n    <div class=\"card-header\" \r\n         style=\"background-color:transparent;\">\r\n        <div class=\"row\">\r\n            <div class=\"header-title col-8\">\r\n                {{header||VISITORS}}\r\n            <\/div>\r\n            <div class=\"header-tag col-4 text-right pl-0\">\r\n                <span class=\"badge\" \r\n                      style=\"background-color:{{tagColor||#1ab394}}; \r\n                             color:#fff;\">\r\n                    {{headerTag||Monthly}}\r\n                <\/span>\r\n                {{reload}}\r\n            <\/div>\r\n        <\/div>    \r\n    <\/div>\r\n    <div class=\"card-body py-0 d-flex align-items-center\">\r\n        <div class=\"stats-icon col-4 py-4 pr-0 d-flex align-items-center\">\r\n            <div id=\"chart-{{id}}\" \r\n                 data-chart-type=\"bar\"\r\n                 data-chart-color=\"{{color||#563d7c}}\"\r\n                 data-raw-data='[\r\n                    {\"dateCreated\":\"28-06-2025 08:05 AM\"},\r\n                    {\"dateCreated\":\"10-07-2025 02:00 PM\"},\r\n                    {\"dateCreated\":\"14-07-2025 11:15 AM\"},\r\n                    {\"dateCreated\":\"05-08-2025 09:00 AM\"}\r\n                 ]'>\r\n            <\/div>\r\n        <\/div>\r\n        <div class=\"stats-info py-3 pr-3 pl-2 col-8\">\r\n            <div class=\"d-flex flex-row align-items-center justify-content-between\" \r\n                 style=\"font-size: 24px; \r\n                        margin-bottom: 10px;\">\r\n                <span>\r\n                    {{value||4}}\r\n                <\/span>\r\n                <span class=\"pl-2\" \r\n                      style=\"font-size:12px; \r\n                             color:{{color||#563d7c}};\">\r\n                    {{subvalue||+8% <i class=\"fas fa-level-up-alt\"><\/i>}}\r\n                <\/span>\r\n            <\/div>\r\n            <h4 style=\"font-size: 12px;\r\n                       color: rgba(0,0,0,.6);\r\n                       margin: 5px 0;\">\r\n                {{title||TOTAL APPROVED BOOKINGS}}\r\n            <\/h4>\r\n        <\/div>\r\n    <\/div>                \r\n    <script>\r\n$(function(){\r\n  const chartId = \"#chart-{{id}}\";\r\n  let rawJson = $(chartId).attr(\"data-raw-data\");\r\n\r\n  try {\r\n    const data = JSON.parse(rawJson);\r\n    const monthTotals = Array(12).fill(0);\r\n\r\n    data.forEach(item => {\r\n      const rawDate = item.dateCreated || item.createdDate || \"\";\r\n      const match = rawDate.match(/^(\\d{2})-(\\d{2})-(\\d{4})/);\r\n      if (match) {\r\n        const month = parseInt(match[2], 10) - 1;\r\n        monthTotals[month]++;\r\n      }\r\n    });\r\n\r\n    const chartConfig = {\r\n      chart: {\r\n        type: 'bar',\r\n        height: '100%',\r\n        sparkline: { enabled: false }\r\n      },\r\n      series: [{\r\n        name: 'Approved Bookings',\r\n        data: monthTotals\r\n      }],\r\n      xaxis: {\r\n        categories: [\"Jan\", \"Feb\", \"Mar\", \"Apr\", \"May\", \"Jun\",\r\n                     \"Jul\", \"Aug\", \"Sep\", \"Oct\", \"Nov\", \"Dec\"]\r\n      },\r\n      colors: [\"{{color||#563d7c}}\"]\r\n    };\r\n\r\n    loadScript(\"{{contextPath}}/js/apexcharts-sparkline.js\", function(){\r\n      new ApexCharts(document.querySelector(chartId), chartConfig).render();\r\n    });\r\n\r\n  } catch (e) {\r\n    $(chartId).html('<p style=\"color:red;\">Invalid chart data format<\/p>');\r\n    console.error(\"Chart error:\", e, rawJson);\r\n  }\r\n\r\n  if ($(chartId).parent().is('[class*=\"col\"]') && $(chartId).parent().width() * 10/16 > $(chartId).parent().parent().height()){\r\n    $(chartId).parent().css({'height':'100%'});\r\n    $(chartId).attr('data-chart-height', '100%');\r\n  }\r\n});\r\n<\/script>\r\n<\/div>\r\n",
